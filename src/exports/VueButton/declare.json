{
  "name": "VueButton",
  "version": "v1.0.0",
  "props": {
    "label": {
      "type": "string",
      "comment": "按钮显示文本",
      "default": ""
    },
    "primary": {
      "type": "boolean",
      "default": true
    },
    "size": {
      "type": [
        "small",
        "medium",
        "large"
      ],
      "default": "medium"
    }
  },
  "events": {
    "onClick": {
      "comment": "点击()=>void"
    }
  },
  "dependencies": [
    "https://cdn.staticfile.org/vue/3.0.9/vue.runtime.global.prod.js"
  ]
}